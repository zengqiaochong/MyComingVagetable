            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.GiftInfoActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.GiftOrderActivity"
            android:screenOrientation="portrait"></activity>
        <activity

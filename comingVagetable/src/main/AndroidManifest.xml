        <activity
            android:name=".activity.LoginActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.RegistActivity"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".activity.OrderConfirm"
            android:screenOrientation="portrait"></activity>
        <activity
